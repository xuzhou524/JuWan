def pods
    pod 'SnapKit'
    pod 'SDWebImage'
    
    pod 'MJRefresh'
    pod 'KVOController'
 
    pod 'MBProgressHUD'
    
    pod 'SwiftyStoreKit'

end

targetsArray = ['JuWan']

targetsArray.each do |t|
  target t do
      pods
  end
end
